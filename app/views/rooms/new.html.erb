<div class="container">
<h1 class="text-center">Enter your room details</h1>
  <div class="row">
    <div class="col-xs-12 col-sm-4 col-sm-offset-4">
      <%= simple_form_for(@room) do |f| %>
        <%= f.error_notification %>

        <%= f.input :title %>
        <%= f.input :address %>
        <%= f.input :city %>
        <%= f.input :capacity %>
        <%= f.input :price %>

        <%= f.button :submit %>
      <% end %>
    </div>
  </div>
</div>
<%= content_for(:after_js) do %>
  <script src="https://maps.googleapis.com/maps/api/js?libraries=places&amp;key=AIzaSyDP6EcPKsYsv2Yv_8QkBDoO72GJtUF01KY"></script>
<% end %>
